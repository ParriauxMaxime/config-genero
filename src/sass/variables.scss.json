{
  "__import": "./material/colors/color_theme",

  "gbc-max-icon-size": "32px",
  "gbc-font-size-ratio": 0.8,
  "gbc-margin-ratio": 0.6,
  "gbc-field-height-ratio": 0.7,
  "gbc-radiobutton-size": "14px",
  "gbc-checkbox-size": "14px",
  "gbc-table-margin": "6px",
  "gbc-combobox-margin": "5px",

  "primary-50": "$mt-blue-50",
  "primary-100": "$mt-blue-100",
  "primary-200": "$mt-blue-200",
  "primary-300": "$mt-blue-300",
  "primary-400": "$mt-blue-400",
  "primary-500": "$mt-blue-500",
  "primary-600": "$mt-blue-600",
  "primary-700": "$mt-blue-700",
  "primary-800": "$mt-blue-800",
  "primary-900": "$mt-blue-900",
  "primary-a100": "$mt-blue-a100",
  "primary-a200": "$mt-blue-a200",
  "primary-a400": "$mt-blue-a400",
  "primary-a700": "$mt-blue-a700",

  "gbc-genero-black": "$mt-grey-900",
  "gbc-genero-blue": "$mt-blue-500",
  "gbc-genero-cyan": "$mt-cyan-500",
  "gbc-genero-darkBlue": "$mt-blue-800",
  "gbc-genero-darkCyan": "$mt-cyan-800",
  "gbc-genero-darkGray": "$mt-grey-800",
  "gbc-genero-darkGreen": "$mt-green-800",
  "gbc-genero-darkMagenta": "$mt-pink-800",
  "gbc-genero-darkOlive": "$mt-light-green-800",
  "gbc-genero-darkOrange": "$mt-orange-800",
  "gbc-genero-darkRed": "$mt-red-800",
  "gbc-genero-darkTeal": "$mt-teal-800",
  "gbc-genero-darkYellow": "$mt-yellow-800",
  "gbc-genero-gray": "$mt-grey-500",
  "gbc-genero-green": "$mt-green-500",
  "gbc-genero-lightBlue": "$mt-blue-100",
  "gbc-genero-lightCyan": "$mt-cyan-100",
  "gbc-genero-lightGray": "$mt-grey-300",
  "gbc-genero-lightGreen": "$mt-green-100",
  "gbc-genero-lightMagenta": "$mt-pink-100",
  "gbc-genero-lightOlive": "$mt-light-green-100",
  "gbc-genero-lightOrange": "$mt-orange-100",
  "gbc-genero-lightRed": "$mt-red-100",
  "gbc-genero-lightTeal": "$mt-teal-100",
  "gbc-genero-lightYellow": "$mt-yellow-100",
  "gbc-genero-magenta": "$mt-pink-500",
  "gbc-genero-olive": "$mt-light-green-500",
  "gbc-genero-orange": "$mt-orange-500",
  "gbc-genero-red": "$mt-red-500",
  "gbc-genero-teal": "$mt-teal-500",
  "gbc-genero-white": "$mt-white",
  "gbc-genero-yellow": "$mt-yellow-500",

  "gbc-primary-text-color": "$gbc-genero-black",
  "gbc-secondary-text-color": "$gbc-genero-white",
  "gbc-primary-background-color": "$mt-grey-50",
  "gbc-secondary-background-color": "$mt-white",
  "gbc-field-background-color": "$mt-white",
  "gbc-field-disabled-background-color": "#EEEEEE",
  "gbc-field-readonly-background-color": "#EEEEEE",
  "gbc-primary-color": "$primary-700",
  "gbc-primary-medium-color": "$primary-500",
  "gbc-primary-light-color": "$primary-100",
  "gbc-secondary-color": "$mt-grey-600",
  "gbc-disabled-color": "$mt-grey-400",
  "gbc-separator-color": "$mt-grey-400",
  "gbc-header-color": "$mt-grey-100",
  "gbc-message-color": "$mt-grey-800",
  "gbc-error-color": "$mt-red-800",
  "gbc-sidebar-always-visible-min-width": "1400px",
  "gbc-sidebar-default-width": "300px",
  "gbc-sidebar-max-width": "800px",
  "gbc-toggle-right-sidebar-min-width": "720px",
  "___gbc-sidebar-default-width": "$gbc-font-size-ratio*350px",
  "gbc-animation-duration": "0.7s",
  "gbc-message-display-time": 10,

  "z-index-Normal": 1,
  "z-index-under-Normal": "$z-index-Normal - 1",
  "z-index-above-Normal": "$z-index-Normal + 1",
  "z-index-TopMenu": "$z-index-Normal * 10",
  "z-index-Toolbar": "$z-index-Normal * 20",
  "z-index-Window": "$z-index-Normal * 30",
  "z-index-Dialog": "$z-index-Normal * 40",
  "z-index-BarsContainer": "$z-index-Normal * 56",
  "z-index-DropDown": "$z-index-Normal * 57",
  "z-index-above-DropDown": "$z-index-DropDown + 1",
  "z-index-Message": "$z-index-Normal * 60",

  "droid-font-path": "\"../lib/fonts/\"",
  "mdi-css-prefix": "zmdi",
  "mdi-font-path": "\"../lib/fonts\"",

  "field-default-height": "$gbc-field-height-ratio * 32px",
  "button-default-width": "$gbc-font-size-ratio * 100px",
  "group-separation-size": "$gbc-margin-ratio * 12",
  "splitter-size": "$group-separation-size * 1px",
  "traditional-mode-letter-spacing": "2px !important",

  "layout-grid-inner-gutter": 3,

  "table-min-width": 60,
  "table-min-pageSize": 3,

  "host-menu-order-upload-icon": 1,
  "host-menu-order-runtime-status": 10,
  "host-menu-order-about": 50,
  "host-menu-order-debug": 100,
  "host-menu-order-run-gwc": 200,
  "host-menu-order-run-gdc": 500,
  "host-menu-order-log-proxy": 1000,
  "host-menu-order-log-vm": 2000,
  "host-menu-order-settings": 5000,
  "host-menu-order-bookmark": 10000,
  "host-menu-order-close": 20000,

  "aui-mutation-watch": false

}
