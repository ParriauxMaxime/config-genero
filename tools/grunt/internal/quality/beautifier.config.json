{
  "html": {
    "braceStyle": "collapse",
    "indentChar": " ",
    "indentScripts": "keep",
    "indentSize": 2,
    "maxPreserveNewlines": 2,
    "preserveNewlines": true,
    "unformatted": ["pre", "code"],
    "wrapLineLength": 140
  },
  "css": {
    "indentChar": " ",
    "indentSize": 2,
    "maxPreserveNewlines": 2
  },
  "js": {
    "braceStyle": "collapse",
    "breakChainedMethods": false,
    "e4x": false,
    "evalCode": false,
    "indentChar": " ",
    "indentLevel": 0,
    "indentSize": 2,
    "indentWithTabs": false,
    "jslintHappy": false,
    "keepArrayIndentation": false,
    "keepFunctionIndentation": false,
    "maxPreserveNewlines": 2,
    "preserveNewlines": true,
    "spaceBeforeConditional": true,
    "spaceInParen": false,
    "unescapeStrings": false,
    "wrapLineLength": 140
  }
}
